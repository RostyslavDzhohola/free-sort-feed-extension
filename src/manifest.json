{
  "manifest_version": 3,
  "name": "Outliers",
  "version": "1.0.1",
  "description": "Filter Instagram Reels to show only outliers with 5Ã— or more views relative to followers.",
  "permissions": [
    "scripting",
    "sidePanel",
    "storage"
  ],
  "host_permissions": [
    "*://*.instagram.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
